defined\") {\n          for (let j = 0; j < dict[i].buttons.length; j++) {\n            dict[i].buttons[j].path = dict[i].path + '/' + dict[i].buttons[j].next;\n          }\n        }\n        element++\n      }\n\n      items = dict.slice(0, length);\n      console.log(items)\n      return items;\n    })\n    return {\n      items\n      \n    }\n  }\n}\n</script>\n\n<style scoped>\n.btn {\n  margin-right: 5px;\n}\n\n</style>\n\n\n"],"mappings":";;AAiDA,SAASA,QAAO,QAAS,YAAW;AACpC;AACA,SAASC,QAAO,QAAS,KAAI;AAE7B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDE,SAAS,EAAE;MACTJ,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDG,GAAG,EAAE;MACHL,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDI,KAAK,WAAAA,MAACR,KAAK,EAAE;IACX,IAAMS,MAAK,GAAIZ,QAAQ,EAAC;IAExB,IAAMa,KAAI,GAAIZ,QAAQ,CAAC,YAAM;MAC3B,IAAIY,KAAI,GAAI,EAAE;MACd,IAAIC,IAAG,GAAI,CAAC;QACVZ,IAAI,EAAE,KAAK;QACXa,OAAO,EAAE,CACP;UACEb,IAAI,EAAE,IAAI;UACVc,IAAI,EAAE;QACR;MAEJ,CAAC,EACD;QACEd,IAAI,EAAEC,KAAK,CAACC,KAAK,CAACF,IAAG,GAAI;MAC3B,CAAC,EACD;QACEA,IAAI,EAAE,IAAI;QACVa,OAAO,EAAE,CAAC;UACRb,IAAI,EAAE,IAAI;UACVc,IAAI,EAAE;QACR,CAAC;MACH,CAAC,EACD;QACEd,IAAI,EAAEC,KAAK,CAACK,OAAO,CAACN,IAAG,GAAI;MAC7B,GAEA;QACEA,IAAI,EAAE,IAAI;QACVa,OAAO,EAAE,CAAC;UACRb,IAAI,EAAE,IAAI;UACVc,IAAI,EAAE;QACR,CAAC;MACH,CAAC,EACD;QACEd,IAAI,EAAEC,KAAK,CAACM,SAAS,CAACP,IAAG,GAAI;MAC/B,CAAC,EACD;QACEA,IAAI,EAAE,IAAI;QACVa,OAAO,EAAE,CAAC;UACRb,IAAI,EAAE,IAAI;UACVc,IAAI,EAAE;QACR,CAAC,EAAE;UACDd,IAAI,EAAE,IAAI;UACVc,IAAI,EAAE;QACR,CAAC;MACH,CAAC,EACD;QACEd,IAAI,EAAE;MACR,EACC;MACD,IAAIe,QAAO,GAAI,CAAC;QACdf,IAAI,EAAE,IAAI;QACVa,OAAO,EAAE,CACP;UACEb,IAAI,EAAE,IAAI;UACVc,IAAI,EAAE;QACR;MAEJ,CAAC,EAAE;QACDd,IAAI,EAAE;MACR,CAAC,CAAC;MAEF,IAAIgB,QAAO,GAAIN,MAAM,CAACO,IAAI,CAACC,KAAK,CAAC,GAAG;MACpC,IAAIC,MAAK,GAAIH,QAAQ,CAACG,MAAK,GAAI,CAAC;MAChC,IAAIH,QAAQ,CAAC,CAAC,KAAK,OAAM,IAAKA,QAAQ,CAAC,CAAC,KAAK,gBAAgB,EAAE;QAC