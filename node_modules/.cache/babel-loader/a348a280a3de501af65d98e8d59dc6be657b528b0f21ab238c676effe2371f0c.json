e[i].path = path;\n        } else {\n          path += '/' + paths.value[i]\n          items.value[i].path = path\n        }\n      }\n      lastIndex.value = items.value.length - 1\n      if ('button' in items.value[lastIndex.value]) {\n        button.value = true;\n        nextPath.value = items.value[lastIndex.value].path + '/' + items.value[lastIndex.value].next;\n        buttonValue.value = items.value[lastIndex.value].button;\n      }\n      console.log(items.value)\n    }\n    return {\n      items,\n      lastIndex,\n      button,\n      buttonValue,\n      nextPath\n    }\n  }\n}\n</script>\n"],"mappings":";AAiDA,SAASA,QAAO,QAAS,YAAW;AACpC;AACA,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAI;AAEnC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,MAAK,GAAIL,QAAQ,EAAC;IACxB,IAAMM,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,IAAMM,SAAQ,GAAIN,GAAG,EAAC;IACtB,IAAMO,KAAI,GAAIP,GAAG,CAAC,EAAE;IACpB,IAAMQ,QAAO,GAAIR,GAAG,EAAE;IACtB,IAAMS,MAAK,GAAIT,GAAG,CAAC,KAAK;IACxB,IAAMU,WAAU,GAAIV,GAAG,EAAE;IACzBC,SAAS,CAAC,YAAM;MACdU,QAAQ,EAAC;MACTC,KAAK,EAAC;IACR,CAAC;IACD,IAAMD,QAAO,GAAI,SAAXA,QAAOA,CAAA,EAAgB;MAC3B,IAAIE,QAAO,GAAIT,MAAM,CAACU,IAAI,CAACC,KAAK,CAAC,GAAG;MACpC,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIH,QAAQ,CAACI,MAAM,EAAED,CAAC,EAAE,EAAE;QACxCT,KAAK,CAACW,KAAK,CAACC,IAAI,CAACN,QAAQ,CAACG,CAAC,CAAC;MAC9B;IACF;IACA,IAAMJ,KAAI,GAAI,SAARA,KAAIA,CAAA,EAAgB;MACxBP,KAAK,CAACa,KAAI,GAAId,MAAM,CAACgB,IAAI,CAACf,KAAI;MAC9B,IAAIS,IAAG,GAAI,EAAE;MACb,KAAK,IAAIE,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIX,KAAK,CAACa,KAAK,CAACD,MAAM,EAAED,CAAC,EAAE,EAAE;QAC3C,IAAIA,CAAA,KAAM,CAAC,EAAE;UACXF,IAAG,GAAI,GAAE,GAAIP,KAAK,CAACW,KAAK,CAACF,CAAC;UAC1BX,KAAK,CAACa,KAAK,CAACF,CAAC,CAAC,CAACF,IAAG,GAAIA,IAAI;QAC5B,OAAO;UACLA,IAAG,IAAK,GAAE,GAAIP,KAAK,CAACW,KAAK,CAACF,CAAC;UAC3BX,KAAK,CAACa,KAAK,CAACF,CAAC,CAAC,CAACF,IAAG,GAAIA,IAAG;QAC3B;MACF;MACAR,SAAS,CAACY,KAAI,GAAIb,KAAK,CAACa,KAAK,CAACD,MAAK,GAAI;MACvC,IAAI,QAAO,IAAKZ,KAAK,CAACa,KAAK,CAACZ,SAAS,CAACY,KAAK,CAAC,EAAE;QAC5CT,MAAM,CAACS,KAAI,GAAI,IAAI;QACn